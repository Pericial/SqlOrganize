<UserControl x:Class="Autocomplete2.UserControls.AutoCompleteTextBoxUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Autocomplete2.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="220">

    <StackPanel  >
        <TextBox  x:Name="autoTextBox"
                  TextChanged="AutoTextBox_TextChanged"
                 Width="200"
                 Height="30"
                 Margin="2"
                  >
        </TextBox>

        <ListBox x:Name="autoList"    
                        Width="200"    
                        Height="100"    
                        SelectionChanged="AutoList_SelectionChanged" />
    </StackPanel>
    
    
    
</UserControl>
