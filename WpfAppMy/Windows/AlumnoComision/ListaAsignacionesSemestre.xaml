<Window x:Class="WpfAppMy.Windows.AlumnoComision.ListaAsignacionesSemestre"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppMy.Windows.AlumnoComision"
        mc:Ignorable="d"
        Title="ListaAsignacionesSemestre" Height="450" Width="800">
    <Grid>
        <DataGrid Name="dataGrid" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Número" Binding="{Binding comision__numero}"></DataGridTextColumn>
                <DataGridTextColumn Header="Sede" Binding="{Binding sede__nombre}"></DataGridTextColumn>
                <DataGridCheckBoxColumn Header="Apertura" Binding="{Binding comision__apertura}"></DataGridCheckBoxColumn>
                <DataGridTextColumn Header="Contact Last Name" Binding="{Binding comision__identificacion}"></DataGridTextColumn>
            </DataGrid.Columns>

        </DataGrid>
    </Grid>
</Window>
